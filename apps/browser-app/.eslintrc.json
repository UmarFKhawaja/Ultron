{
  "extends": [
    "plugin:@nx/react",
    "../../.eslintrc.json"
  ],
  "ignorePatterns": [
    "!**/*"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx"
      ],
      "rules": {
        "eslint-disable-next-line no-empty-pattern": [
          "off"
        ],
        "@typescript-eslint/no-empty-function": [
          "off"
        ],
        "@typescript-eslint/no-inferrable-types": [
          "off"
        ],
        "@typescript-eslint/no-empty-interface": [
          "off"
        ]
      }
    },
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.js",
        "*.jsx"
      ],
      "rules": {}
    }
  ]
}
